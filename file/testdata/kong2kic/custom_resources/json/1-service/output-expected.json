{
    "kind": "KongIngress",
    "apiVersion": "configuration.konghq.com/v1",
    "metadata": {
        "name": "example-service",
        "creationTimestamp": null,
        "annotations": {
            "kubernetes.io/ingress.class": "kong"
        }
    },
    "proxy": {
        "protocol": "http",
        "path": "/v1",
        "retries": 5,
        "connect_timeout": 5000,
        "read_timeout": 60000,
        "write_timeout": 60000
    }
}{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "example-service",
        "creationTimestamp": null,
        "annotations": {
            "konghq.com/override": "example-service"
        }
    },
    "spec": {
        "ports": [
            {
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80
            }
        ],
        "selector": {
            "app": "example-service"
        }
    },
    "status": {
        "loadBalancer": {}
    }
}